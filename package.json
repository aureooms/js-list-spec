{
  "name": "@aureooms/js-list-spec",
  "description": "List ADT specification code bricks for JavaScript",
  "homepage": "http://aureooms.github.io/js-list-spec/",
  "main": "lib/index.js",
  "version": "8.0.3",
  "repository": {
    "url": "https://github.com/aureooms/js-list-spec.git",
    "type": "git"
  },
  "keywords": [
    "bricks",
    "javascript",
    "js",
    "list",
    "specification"
  ],
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "test": "ava",
    "esdoc": "esdoc",
    "prepare": "npm run build"
  },
  "author": "aureooms",
  "devDependencies": {
    "aureooms-node-package": "^6.0.1",
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.5",
    "@babel/register": "7.9.0",
    "ava": "3.8.1",
    "coveralls": "3.0.11",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.2.1",
    "nyc": "15.0.1"
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-list-spec/issues"
  },
  "dependencies": {},
  "license": "AGPL-3.0",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-async-generator-functions"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "timeout": "1m"
  },
  "files": [
    "lib"
  ]
}
